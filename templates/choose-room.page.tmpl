{{template "base" .}}

{{define "content"}}
    <div class="container">
        <div class="col">
            <h1>Choose a room</h1>
            {{$rooms:= index .Data "rooms"}}
            <ul>
                {{range $rooms}}
                    <li> <a href="choose-room/{{.ID}}">{{.RoomName}}</a></li>
                {{end}}
            </ul>
        </div>
    </div>
{{end}}